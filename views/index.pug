extends ./includes/template
block style
	link(href='/css/style.css', rel='stylesheet')
block content
	body
		.wrapper
			header#header
				// Nav Bar
				nav.navbar.navbar-static-top(role='navigation')
					.navbar-header
						button.navbar-toggle.collapsed(type='button', data-toggle="collapse",data-target="#navbar", aria-expanded='false', aria-controls='navbar')
							span.sr-only Toggle navigation
							span.icon-bar
							span.icon-bar
							span.icon-bar
					#navbar.navbar-collapse.nav-collapse.collapse
						ul.nav.navbar-nav.navbar-left
							li.active
								a(href='#') HOME
							li
								a(href='#about') ABOUT US
							li.dropdown
								a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
									| ROOMS 
								ul.dropdown-menu
									li
										a(href='#room_details') ROOM DETAILS
									li
										a(href='#rooms2') ROOMS 2
									li
										a(href='#payment') PAYMENT
							li
								a(href="#conf_room") CONFERENCE ROOM    
			
						ul.nav.navbar-nav.navbar-right
							li
								a(href='#wed_hall') WEDDING HALL
							li
								a(href='#restaurant') RESTAURANT
							li.dropdown
								a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') BLOG
								ul.dropdown-menu
									li: a(href="#single_blog") BLOG SINGLE
							li
								a(href="#contact") CONTACT
					a.brand(href='#')
						img(src="img/logo.png", height=130, width=220)
			.jumbotron
				//Carousel
				.carousel.slide#mycarousel(data-ride="carousel")
					.carousel-inner(role="listbox")
						.item.active
							img.img-responsive(src="/img/slide_1.jpg", alt="slide_1")
							.carousel-caption
								p.item1 RESERVE A ROOM FOR
								p.item2 FAMILY VACATION
						.item
							img.media-object(src="img/slide_5.jpg", alt="slide_5")
							.carousel-caption
								p.item1 RESERVE A ROOM FOR
								p.item2 FAMILY VACATION
						.item
							img.media-object(src="img/slide_3.jpg", alt="slide_3")
							.carousel-caption
								p.item1 RESERVE A ROOM FOR
								p.item2 FAMILY VACATION
			main.content
				.container-fluid
					.row.reserveRow
						.col-xs-offset-1.col-xs-8
							p.reserveText Reserve a room for Conference - Lorem ipsum dolor sit amet
						.col-xs-2
							a.btn.btn-block#reserveButton(type="button")
								| RESERVE NOW
					.row
						.col-xs-12
							p.center_text A FEW WORDS
		footer.footer
			// Nav footer
			nav.navbar.nav_footer(role='navigation')
				.container
					#navbar_footer
						ul.nav.navbar-nav.navbar-left
							li.active
								a(href='#') HOME
							li
								a(href='#about') ABOUT US
							li.dropdown
								a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
									| ROOMS 
								ul.dropdown-menu
									li
										a(href='#room_details') ROOM DETAILS
									li
										a(href='#rooms2') ROOMS 2
									li
										a(href='#payment') PAYMENT
							li
								a(href="#conf_room") CONFERENCE ROOM    
			
						ul.nav.navbar-nav.navbar-right
							li
								a(href='#wed_hall') WEDDING HALL
							li
								a(href='#restaurant') RESTAURANT
							li.dropdown
								a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') BLOG
								ul.dropdown-menu
									li: a(href="#single_blog") BLOG SINGLE
							li
								a(href="#contact") CONTACT
					a.brand_footer(href='#')
						img(src="img/f-logo.png", height=41, width=132)
			.container-fluid(style="background-color: #C2A476;")
				.row.row-content#bottom
					.col-xs-offset-1.col-xs-6
						p#rights Â© RangeRelaxe. All rights reserved. Design by GalliaSoft
					.col-xs-push-2.col-xs-3
						a.btn.btn-social-icon.btn-facebook.social_btn(href='http://www.facebook.com/profile.php?id=')
							span.fa-stack
								i.fa.fa-circle-thin.fa-stack-2x(style="color:white;")
								i.fa.fa-facebook.fa-stack-1x(style="color:white;")
						a.btn.btn-social-icon.btn-twitter.social_btn(href='http://twitter.com/')
							span.fa-stack
								i.fa.fa-circle-thin.fa-stack-2x(style="color:white;")
								i.fa.fa-twitter.fa-stack-1x(style="color:white;")
						a.btn.btn-social-icon.btn-linkedin.social_btn(href='http://www.linkedin.com/in/')
							span.fa-stack
								i.fa.fa-circle-thin.fa-stack-2x(style="color:white;")
								i.fa.fa-linkedin.fa-stack-1x(style="color:white;")
						a.btn.btn-social-icon.btn-youtube.social_btn(href='http://youtube.com/')
							span.fa-stack
								i.fa.fa-circle-thin.fa-stack-2x(style="color:white;")
								i.fa.fa-youtube.fa-stack-1x(style="color:white;")
						a.btn.btn-social-icon.social_btn(href='http://vk.com/')
							span.fa-stack
								i.fa.fa-circle-thin.fa-stack-2x(style="color:white;")
								i.fa.fa-envelope-o.fa-stack-1x(style="color:white;")
block script
	script(type="text/javascript", src='/js/carousel.js')